/* pages/allType/allType.wxss */
@import '../util/publicCss.wxss';
.main{
  width: 100%;
}
.top{
  height: 108rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  background-color: #fff;
  border-top:1rpx;
  border-top-style: solid;
  border-top-color: rgb(223,223,221);
}
.top_left{
  height: 24rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:flex-start;
}
.top_leftL,.top_leftL>image{
  width: 39rpx;
  height: 39rpx;
  
}
.top_leftL{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:center;
  margin-right: 11rpx;
}
.top_leftR{
  height: 39rpx;
  line-height: 39rpx;
  font-size: 32rpx;
  color:rgb(0,0,0);
  
}
.top_right,.top_right>image{
  width: 15rpx;
  height: 24rpx;
}
.top_right{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:center;
}
/* 技能 */

.type_Skill{
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 37rpx 23rpx;
}
.type_SkillTxt{
  font-size: 30rpx;
  color: rgb(102,102,102);
}
.type_SkillContent{
  padding:50rpx 0 48rpx 0;
  
}
.skill_Item{
  width: 175rpx;
  height: 177rpx;
  display: inline-block; 
  margin: 0 28.125rpx 0 28.125rpx;
}
.skill_ItemOther{
   width: 175rpx;
  height: 177rpx;
  display: inline-block; 
  margin: 69rpx 28.125rpx 0 28.125rpx;
}
.skill_ItemT{
  width: 175rpx;
  height: 177rpx;
  display: flex;
 
  flex-direction: column;
  align-items: center;
}
.skill_ItemT>image{
  width: 104rpx;
  height: 104rpx;
}
.skill_ItemTxt{
  height: 26rpx;
  font-size: 26rpx;
  margin-top:24rpx;
  color: rgb(51,51,51);
}