/* pages/orderCenter/choosePsnPage.wxss */
@import '../util/publicCss';
.first_item{
   width: 100vw;
  height: 193rpx;
 
  border-top: 1rpx;
  border-top-color: rgb(223,223,221);
  border-top-style: solid;
   border-bottom: 1rpx;
  border-bottom-style: solid;
  border-bottom-color: rgb(223,223,221);
  background-color: rgb(255,255,255);
}
.item_main{
  width: 100vw;
  height: 193rpx;
  border-bottom: 1rpx;
  border-bottom-style: solid;
  border-bottom-color: rgb(223,223,221);
  background-color: rgb(255,255,255);
}
.item_content{
  height: 128rpx;
  padding: 34rpx 23rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.item_contentL{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start; 
}

.item_image{
  margin-right: 12rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.item_image,.item_image>image{
width:124rpx;
height:124rpx;
border-radius: 6rpx;
}
.item_txtName{
  height: 30rpx;
  font-size: 30rpx;
  color:rgb(51,51,51);
  line-height: 30rpx;
  margin-bottom: 19rpx;

}
.item_sex{
  width: 102rpx;
  height: 36rpx;
  border-radius: 18rpx;
 
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 16rpx;
}
.item_sexN{
  background-color:rgb(108,127,235);
}
.item_sexW{
  background-color: rgb(255,64,129);  
}
.item_sexL{
  margin-left: 22rpx;
  margin-right: 5rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.item_sexL,.item_sexL>image{
  width: 24rpx;
  height: 25rpx;
}
.item_sexR{
  height: 24rpx;
  font-size: 24rpx;
  color: rgb(255,255,255);
  line-height: 24rpx;
}
.item_qinming{
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: rgb(51,51,51);
}

.item_time{
  height: 24rpx;
  font-size: 24rpx;
  color: rgb(102,102,102);
  line-height: 24rpx;
  margin-bottom: 25rpx;
    text-align: right;
}
.item_num{
  height: 24rpx;
  font-size: 24rpx;
  color: rgb(102,102,102);
  line-height: 24rpx;
  margin-bottom: 28rpx;
    text-align: right;
}
.item_money{
  height: 30rpx;
  line-height: 30rpx;
  color:rgb(255,51,71);
    text-align: right;
}
.item_money::after{
  content: '/小时';
  font-size: 30rpx;
  color:rgb(51,51,51);
}
.Footer{
  height: 100vh;
  background: rgba(0, 0 ,0, 0.05);
 
  position: relative;
}
.FMain{
  background: #fff;
  padding: 20rpx;
  position: absolute;
  bottom: 0,
}
.Footer_Item{
  height: 68rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  margin-bottom: 20rpx;
}

.Footer_lab{
  font-size: 30rpx;
  color: #333333;
}
.Footer_Val{
  font-size: 24rpx;
  color: #666666;
}
.FooterBtn{
  width: 700rpx;
  height: 68rpx;
  margin: 0 auto;
  background: rgb(108,127,235);
  border-radius: 6rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  line-height: 68rpx;
}
.canleBtn{
   width: 700rpx;
  height: 68rpx;
 
  background: rgb(108,127,235);
  border-radius: 6rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  line-height: 68rpx;
  position: absolute;
  bottom: 10rpx;
  left: 0;
  right: 0;
   margin: 0 auto;
}
/* 无数据Css */
.noData{
width: 100vw;
height: 100%;
}

.noData_Img{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  margin-top: 220rpx;
}
.noData_Img,.noData_Img>image{
  width: 136rpx;
  height: 136rpx;
}
.noData_Txt{
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 32rpx;
  color: #777777;
  margin-top: 65rpx;
}