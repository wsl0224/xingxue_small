/* pages/skillPage/skillPage.wxss */
.body_main{
  height:85vh;
}
.main{
  padding: 0 24rpx;
}
.top{
  height: 124rpx;
  padding: 34rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.psnInfo{
  height: 124rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.psnInfo_img{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-right: 25rpx;
}
.psnInfo_img,.psnInfo_img>image{
  width: 124rpx;
   height: 124rpx;
   border-radius: 6rpx;
}
.psnInfoRT{
   display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 40rpx;
}
.psnInfo_name{
  font-size: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: rgb(51,51,51);
  margin-right: 23rpx;
}
.psnInfo_sex{
  width: 102rpx;
  height: 36rpx;
  border-radius: 18rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.psnInfo_sexN{
  background-color:rgb(108,127,235);
}
.psnInfo_sexW{
  background-color: rgb(255,64,129);  
}

.psnInfo_sexImg{
  margin-left: 22rpx;
  margin-right: 5rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.psnInfo_sexImg,.psnInfo_sexImg>image{
  width: 24rpx;
  height: 24rpx;
}
.psnInfo_sexTxt{
  height: 24rpx;
  font-size: 24rpx;
  color: rgb(255,255,255);
  line-height: 24rpx;
}
.psnInfo_addr{
  height: 22rpx;
  line-height: 22rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 33rpx;
}
.psnInfo_addrImg{
  margin-right: 7rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.psnInfo_addrImg,.psnInfo_addrImg>image{
  width: 14rpx;
  height: 16rpx;
}
.psnInfo_addrTxt{
  height: 22rpx;
  line-height: 22rpx;
  font-size: 22rpx;
  color: rgb(51,51,51);
}
.topR{
  height: 124rpx;
}
.guanzhu{
  width: 86rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
    border-radius: 8rpx;
  border:1rpx;
  border-style: solid;
  border-color: rgb(134,178,249);
  color: rgb(134,178,249);
  font-size: 24rpx;
}
.photo{
  height: 540rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.photoImg{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.photoImg,.photoImg>image{
  width: 520rpx;
  height: 540rpx;
}
.photoR{
  width: 164rpx;
  height: 540rpx;
  display: flex;
  flex-direction: column;
}

.photoR_item{
   display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-bottom: 24rpx;
}
.photoR_itemLast{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.photoR_item,.photoR_itemLast,.photoR_item>image,.photoR_itemLast>image{
   width: 164rpx;
   height: 164rpx;
}
.skill{
  height: 192rpx;
  margin-top: 43rpx;
  border-bottom: 1rpx;
  border-bottom-style: solid;
  border-bottom-color: rgb(223,223,221);
}
.skillTitle{
  height: 30rpx;
  font-size: 30rpx;
  color: rgb(51,51,51);
}
.skillMoney{
  height: 30rpx;
  line-height: 30rpx;
  color: rgb(255,51,71);
  font-size: 30rpx;
  text-align: right;
  margin-top: 38rpx;
  margin-bottom: 31rpx;
}
.skillNP{
   display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.skillNPL{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 11rpx;
}
.skillNPL_IMG{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-right: 12rpx;
}
.skillNPL_IMG,.skillNPL_IMG>image{
  width: 24rpx;
  height: 24rpx;
}
.skillNPL_txt{
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color:rgb(102,102,102);
}
/* 评分Css */
.pingfen{
  height: 30rpx;
  margin:10rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.pingfenList{
  height: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-right:10rpx;
}
.pingfenImg{
    display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}
.pingfenImg,.pingfenImg>image{
  width: 20rpx;
  height: 22rpx;

}
.pingfenTxt{
  font-size: 22rpx;
  height: 22rpx;
  line-height: 24rpx;
  color: rgb(255,185,36);
}

.skillMusic{
  width: 174rpx;
  height: 45rpx;
  border:1rpx;
  border-style: solid;
  border-color: rgb(134,178,249);
  border-radius: 21rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
 
}

.skillML{
   display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-left: 51rpx;
  margin-right: 8rpx;
}
.skillML,.skillML>image{
  width: 24rpx;
  height: 24rpx;
}
.skillM_Time{
  height: 24rpx;
  line-height: 24rpx;
  color: rgb(102,102,102);
  font-size: 24rpx;
}
.skillM_Time::after{
  content: "'";
    color: rgb(102,102,102);
}


.skillContxt{
  height: 28rpx;
  margin-top: 34rpx;
  font-size: 28rpx;
  margin-bottom: 89rpx;
  color: rgb(102,102,102);
}
/* footer */
.footer{
  border-top:1rpx;
  border-top-style: solid;
  border-top-color: rgb(223,223,221);
  padding: 0 24rpx;
  height: 146rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  bottom: 0rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.footerL{
  width: 268rpx;
  height: 86rpx;
  line-height: 86rpx;
  border: 1rpx;
  border-style: solid;
  border-color: rgb(134,178,249);
  border-radius: 8rpx;
  text-align: center;
  font-size: 36rpx;
  color: rgb(134,178,249);
}
.footerR{
  width: 402rpx;
  height: 86rpx;
  line-height: 86rpx;
  background-color: rgb(134,178,249);
  border-radius: 8rpx;
  text-align: center;
  font-size: 36rpx;
  color: rgb(255,255,255);
}