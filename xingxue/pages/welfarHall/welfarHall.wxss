/* pages/welfarHall/welfarHall.wxss */
@import '../util/publicCss.wxss';
.main{
 
  border-top: 1rpx;
  border-top-color:#dfdfdf;
  border-top-style: solid;

}
.Item{
  width: 624rpx;
  height: 229rpx;
  padding: 23rpx 23rpx 23rpx 23rpx;
  display: flex;
  flex-direction: row;
  align-items: center; 
  justify-content: space-between; 
  margin: 0 auto;
  background: #fff;
  margin-top: 30rpx;
  box-shadow: 0 0 10rpx 1rpx rgba(255,255,255,0.5);
  background-image: url('https://app.xingxue.vip/icon/welfarHallBG.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.url_ItemL_B,{
  display: flex;
  flex-direction: row;
  align-items: center; 
  justify-content: center; 

}


.url_ItemL_B,.url_ItemL_B>image{
   width: 144rpx;
  height:144rpx;
  border-radius: 4rpx;
   box-shadow: 0 0 10rpx 1rpx rgba(255,255,255,0.5);
}
.url_ItemR{
  width: 100%;
  height: 229rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-end; 
  justify-content: space-between; 
}

.item_txt{
  text-align: right;
  font-size: 32rpx;
  color: #333333;
}
.url_ItemRBtn{
  font-size: 26rpx;
  color: #86b2f9;
  width: 172rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  border:1rpx solid #86b2f9;
  border-radius: 4rpx;
  
}