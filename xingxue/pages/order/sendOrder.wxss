/* pages/order/sendOrder.wxss */
OneLeverScroll{
  height: 41rpx;
}
.main{
  border-top:1rpx;
  border-top-style: solid;
  border-top-color: rgb(223,223,221);
  padding:32rpx 22rpx;
}
/* 一级 */
.OneLeverScroll{
  height: 50rpx;
}
.OneLeverTar{
 height:50rpx;
 width:100%; 
 display: flex; 
 display: -webkit-box; 
 flex-direction: row;
}
.OneTar_Item{
  width: 130rpx;
  height: 41rpx;
  flex: 1;
  color: rgb(107,107,107);
 font-size: 32rpx;
 margin-right: 51rpx;
 text-align: center;
  padding-bottom: 7rpx;
}
.OneTar_Item.active{
  color: rgb(134,178,249);
  font-size: 32rpx;
  border-bottom: 3rpx;
  border-bottom-color: rgb(134,178,249);
  border-bottom-style: solid;
}
/* 二级 */
.TwoLeverScroll{
  height: 52rpx;
  margin: 37rpx 0 34rpx 0; 
}
.TwoLeverTar{
  height:52rpx;
 width:100%; 
 display: flex; 
 display: -webkit-box; 
 flex-direction: row;
}
.TwoTar_Item{
  flex: 1;
  width: 166rpx;
  height: 52rpx;
 margin-right: 20rpx;
 border-radius: 2rpx;
 background-color: rgb(237,237,237);
 text-align: center;
 font-size: 32rpx;
 line-height: 52rpx;
}
.TwoTar_Item.active{
  color: #fff;
  background-color: rgb(134,178,249);
}
/* map中心点 */
.mapMid{
  width: 308rpx;
  height: 225rpx;
  background-color:transparent;
  position: absolute;
  bottom: 495rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
 

}
.mapMidTop{
  width:288rpx;
  height: 103rpx;

}
.mapMidBottom{
   display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.mapMidBottom,.mapMidBottom>cover-image{
  width: 38rpx;
  height: 89rpx;
}